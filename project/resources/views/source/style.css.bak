@media screen and  (max-width: 400px) {
	.buttonlink.fb a:before,.buttonlink.google a:before{left:15px;}
	.buttonlink a{ font-size: 15px;}
	h1.title{font-size:24px;}
	footer .inner{padding-right:10px;}
	.payconfirmation{ padding:30px 15px;}
	body{font-size:16px;}
	.scantopaytext{font-size:18px;}
	.payconfirmation .amount{margin-right:20px;}
	.paycomplete .amount { margin-right: 20px;}
	.paycomplete .comtitle{font-size:16px;}
	.paycomplete{padding:30px 10px 15px;font-size:14px;}
	footer .flyrocket{width:55px;height:55px;    top: -18px;}
	footer .flyrocket i{font-size:30px;line-height:47px;}
	.list .detail>td{padding-left:15px;}
	.financetip .col-xs-6{width:100%;}
	.financetip img{margin-bottom:15px;}
	#filter select{
		width: 140px;
		padding: 7px 10px;	
	}
	.tarn_detailblock .rightside a{font-size:20px;    margin: 0 3px;}
	.tarn_detailblock .rightside .amount{font-size:28px;
    white-space: nowrap;}
	.loyaltilinkblock .giftcardlink,.loyaltilinkblock .loyaltylink{font-size:12px;}
}
@media screen and  (max-width: 320px) {
	body,table{    font-size: 14px;}
	.tarn_detailblock .rightside .amount{font-size:24px;}
	#search_transaction_advanced #from,#search_transaction_advanced #to{width:90px;margin-right:1px;}
	.loyaltilinkblock .giftcardlink{width:55%;}
	.loyaltilinkblock .loyaltylink{
		width:43%;	
	}
}