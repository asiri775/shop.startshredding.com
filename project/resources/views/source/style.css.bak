@font-face {
    font-family: 'AvenirLTStd-Book';
    src: url('../fonts/AvenirLTStd-Book.woff2') format('woff2'),
         url('../fonts/AvenirLTStd-Book.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'AvenirLTStd-Heavy';
    src: url('../fonts/AvenirLTStd-Heavy.woff2') format('woff2'),
         url('../fonts/AvenirLTStd-Heavy.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'AvenirLTStd-Light';
    src: url('../fonts/AvenirLTStd-Light.woff2') format('woff2'),
         url('../fonts/AvenirLTStd-Light.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'AvenirLTStd-Medium';
    src: url('../fonts/AvenirLTStd-Medium.woff2') format('woff2'),
         url('../fonts/AvenirLTStd-Medium.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}
@font-face {font-family: "AvenirLTStd-Black";
	src: url("../fonts/AvenirLTStd-Black.woff2") format("woff2"),
    url("../fonts/AvenirLTStd-Black.woff") format("woff");
	  font-weight: normal;
   font-style: normal;
}

body,html{font-family: 'AvenirLTStd-Book'; background: #fff;font-size:17px;min-height: 100%;word-wrap: break-word;background-color:#fbfbfb;    color: #333;}
body{overflow-x:hidden;width:100%;position:relative;    padding-bottom: 30px;}
